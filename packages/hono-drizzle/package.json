{
  "name": "hono-drizzle",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "type": "module",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "tsc --build tsconfig.json",
    "format": "prettier --check \"{src,test}/**/*.ts\"",
    "format:fix": "prettier --write \"{src,test}/**/*.ts\"",
    "lint": "eslint --cache \"{src,test}/**/*.ts\"",
    "lint:fix": "eslint --cache \"{src,test}/**/*.ts\" --fix",
    "prebuild:drizzle-kit": "rimraf dist-drizzle-kit",
    "build:drizzle-kit": "tsc -p tsconfig.drizzle-kit.json",
    "db:generate": "yarn build:drizzle-kit && dotenv -e .env.local drizzle-kit generate",
    "db:migrate": "yarn build:drizzle-kit && dotenv -e .env.local drizzle-kit migrate",
    "db:push": "yarn build:drizzle-kit && dotenv -e .env.local drizzle-kit push",
    "start:local": "node --env-file=.env.local dist/entryPoint/primary/management/web/main.js",
    "test": "vitest --typecheck",
    "test:cov": "vitest --coverage",
    "test:db": "TEST_TYPE=db; dotenv -e .env.local vitest --no-file-parallelism",
    "test:db:generate": "yarn build:drizzle-kit && dotenv -e .env.test drizzle-kit generate",
    "test:db:migrate": "yarn build:drizzle-kit && dotenv -e .env.test drizzle-kit migrate",
    "test:db:push": "yarn build:drizzle-kit && dotenv -e .env.test drizzle-kit push"
  },
  "dependencies": {
    "@hono/node-server": "1.19.5",
    "date-fns": "4.1.0",
    "dotenv": "17.2.3",
    "drizzle-orm": "0.44.6",
    "hono": "4.9.12",
    "neverthrow": "8.2.0",
    "pg": "8.16.3",
    "remeda": "2.32.0",
    "typed-inject": "5.0.0",
    "ulidx": "2.4.1",
    "yargs": "18.0.0",
    "zod": "3.25.76"
  },
  "devDependencies": {
    "@eslint/compat": "1.4.0",
    "@stylistic/eslint-plugin-js": "4.4.1",
    "@types/node": "22.18.6",
    "@types/pg": "8.15.5",
    "@vitest/coverage-v8": "3.2.4",
    "@vitest/ui": "3.2.4",
    "dotenv-cli": "^10.0.0",
    "drizzle-kit": "0.31.5",
    "eslint-plugin-neverthrow": "1.1.4",
    "eslint-plugin-no-relative-import-paths": "1.6.1",
    "eslint-plugin-simple-import-sort": "12.1.1",
    "rimraf": "6.0.1",
    "tsx": "4.20.6",
    "typescript": "5.9.3",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "3.2.4"
  }
}
